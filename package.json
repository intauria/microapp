{
  "name": "microapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "microapp:copy:readme": "cpr README.md dist/libs/microapp/README.md",
    "microapp:copy:license": "cpr LICENSE dist/libs/microapp/LICENSE",
    "microapp:copy": "npm run microapp:copy:readme && npm run microapp:copy:license",
    "microapp:build:production": "nx run microapp:build:production --skip-nx-cache",
    "postmicroapp:build:production": "npm run microapp:copy",
    "prerelease": "cd libs/microapp && npm version prerelease --preid beta",
    "release": "npm run microapp:build:production",
    "release:npm": "npm run release",
    "postrelease:npm": "cd dist/libs/microapp && npm publish --access public --tag latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nrwl/angular": "13.8.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@nrwl/cli": "13.8.0",
    "@nrwl/cypress": "13.8.0",
    "@nrwl/eslint-plugin-nx": "13.8.0",
    "@nrwl/jest": "13.8.0",
    "@nrwl/linter": "13.8.0",
    "@nrwl/tao": "13.8.0",
    "@nrwl/workspace": "13.8.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "cpr": "^3.0.1",
    "cypress": "^9.1.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "ng-packagr": "^13.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  }
}
